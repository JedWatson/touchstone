{
  "name": "touchstonejs",
  "version": "0.6.0",
  "description": "A Mobile App Framework built with ReactJS",
  "author": "Jed Watson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/touchstonejs/touchstonejs.git"
  },
  "bugs": {
    "url": "https://github.com/touchstonejs/touchstonejs/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "ui",
    "mobile",
    "hybrid"
  ],
  "main": "lib/index.js",
  "files": [
    "lib/",
    "less"
  ],
  "scripts": {
    "build": "gulp clean:lib && NODE_ENV=production gulp build:lib",
    "build:dev": "gulp clean:lib && gulp build:lib",
    "build:site": "gulp clean:site && NODE_ENV=production gulp build:site",
    "lint": "happiness",
    "start": "gulp site",
    "test": "npm run lint",
    "watch": "gulp watch:lib"
  },
  "standard": {
    "ignore": [
      "**/lib/**",
      "**/__build/**"
    ]
  },
  "dependencies": {
    "blacklist": "^1.1.2",
    "classnames": "^2.2.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-container": "^0.3.0",
    "react-tappable": "^0.7.1",
    "tween.js": "^0.14.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-plugin-object-assign": "^1.2.1",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "del": "^2.0.2",
    "elemental": "^0.5.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.2.1",
    "gulp-bump": "^1.0.0",
    "gulp-connect": "^2.2.0",
    "gulp-gh-pages": "^0.5.2",
    "gulp-git": "^1.4.0",
    "gulp-less": "^3.0.3",
    "gutil": "^1.6.4",
    "happiness": "^1.0.7",
    "react-router": "^1.0.0-rc3",
    "vinyl-source-stream": "^1.1.0"
  }
}
